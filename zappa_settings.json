{
    "dev": {
        "aws_region": "ap-northeast-2",
        "django_settings": "config.settings",
        "profile_name": "default",
        "project_name": "lukuku-test-plus",
        "runtime": "python3.10",
        "s3_bucket": "lukuku-plus-assets",
        "domain": "lukuku-plus-api.lukuku.co",
        "cloudwatch_log_level": "INFO",
        "cloudwatch_data_trace": true,
        "cloudwatch_metrics_enabled": true,
        "certificate_arn": "arn:aws:acm:us-east-1:259057628405:certificate/5c7df3c4-fc23-478d-878c-a45e153c6b31",
        "exception_handler": "services.handler.unhandled_exceptions",
        "cors": {
            "allowed_headers": [
                "Content-Type",
                "Cache-Control",
                "X-Requested-With",
                "X-Amz-Date",
                "Authorization",
                "X-Api-Key",
                "X-Amz-Security-Token"
            ],
            "allowed_methods": [
                "DELETE",
                "GET",
                "HEAD",
                "OPTIONS",
                "PATCH",
                "POST",
                "PUT"
            ],
            "allowed_origin": "*"
        }
    }
}